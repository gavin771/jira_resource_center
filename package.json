{
  "name": "qw-resource-center",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gavin771.github.io/",
  "dependencies": {
    "express": "~4.15.4",
    "formidable": "~1.1.1",
    "gh-pages": "~1.0.0",
    "moment": "~2.18.1",
    "react": "~15.6.1",
    "react-datepicker": "~0.53.0",
    "react-dom": "~15.6.1",
    "react-router-dom": "~4.1.2",
    "react-scripts": "1.0.10",
    "semantic-ui-css": "~2.2.12",
    "semantic-ui-react": "~0.71.3"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start:dev": "concurrently \"yarn start:server-dev\" \"yarn build && yarn start:client\"",
    "start:client": "react-scripts start",
    "start:server-dev": "nodemon --exec babel-node ./server/app.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-without-watch": "CI=true npm test",
    "eject": "react-scripts eject",
    "precommit": "lint-staged && npm run test-without-watch"
  },
  "devDependencies": {
    "babel-eslint": "~7.2.3",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-preset-es2015": "~6.24.1",
    "chromedriver": "2.29.0",
    "concurrently": "~3.5.0",
    "enzyme": "~2.9.1",
    "husky": "~0.14.3",
    "lint-staged": "~4.0.3",
    "nightwatch": "~0.9.16",
    "prettier": "~1.5.3",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "selenium-server-standalone-jar": "~3.4.0",
    "standard": "~10.0.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --no-semi",
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "expect"
    ]
  }
}